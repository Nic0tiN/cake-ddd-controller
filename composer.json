{
  "name": "ngirardet/cake-ddd-controller",
  "description": "CakePHP Domain Driven Design Rest Controller",
  "homepage": "https://github.com/Nic0tiN/cake-ddd-controller",
  "type": "library",
  "authors": [
    {
      "name": "N. Girardet",
      "homepage": "https://github.com/Nic0tiN"
    }
  ],
  "license": [false],
  "require": {
    "php": "8.1",
    "cakephp/cakephp": "^4.4|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "CakeDDD\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CakeDDD\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --bootstrap ./tests/bootstrap.php ./tests"
  },
  "prefer-stable": true
}